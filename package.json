{
  "name": "freshmint",
  "version": "0.0.17-alpha.2",
  "description": "The minty fresh way to launch an NFT project.",
  "contributors": [
    "Mackenzie Kieran",
    "Peter Siemens <peterjsiemens@gmail.com>"
  ],
  "license": "MIT",
  "repository": "github:packagelabs/freshmint",
  "keywords": [
    "nft",
    "blockchain"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": "./dist/lib/index.js",
    "./crypto": "./dist/lib/crypto/index.js"
  },
  "bin": {
    "fresh": "dist/cli/index.js"
  },
  "scripts": {
    "test": "jest --runInBand",
    "prebuild": "tsc --build --clean",
    "build": "tsc",
    "postbuild": "npm run copy-cli-templates && npm run copy-lib-templates",
    "dev": "tsc --watch",
    "copy-cli-templates": "cpx --include-empty-dirs \"src/cli/templates/**\" dist/cli/templates",
    "copy-lib-templates": "cpx --include-empty-dirs \"src/lib/templates/**\" dist/lib/templates",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@onflow/decode": "0.0.11",
    "@onflow/fcl": "^1.2.0",
    "@onflow/types": "^1.0.3",
    "@onflow/util-address": "0.0.0",
    "@tuplo/envsubst": "^1.15.0",
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "csv-parse": "^4.16.3",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.1",
    "elliptic": "^6.5.4",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.2",
    "js-yaml": "^4.1.0",
    "nft.storage": "^3.3.0",
    "ora": "^5.4.1",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "progress": "^2.0.3",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.2",
    "@types/pouchdb": "^6.4.0",
    "@types/progress": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cpx": "^1.5.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
